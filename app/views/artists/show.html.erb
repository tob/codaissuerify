<div class="row">
  <div class="col-md-3">
    <h4><%= @artist.name %></h4>
    <%= image_tag(@artist.image_url) =%>
    <%= @artist.id =%>
    <%= link_to "Delete", artist_path(@artist), class: "btn btn-default", method: :delete, data: { confirm: "Are you sure?" } %>
  </div>
</div>
<div class="row">
  <%= render 'songs/list_songs' %>
</div>

<div class="panel-body">
  <%= render 'songs/form' %>
</div>
